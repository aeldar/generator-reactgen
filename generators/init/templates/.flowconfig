[ignore]
.*/node_modules/react-motion/.*
.*/node_modules/fbjs/.*
.*/node_modules/editions/.*

[include]

[libs]

[options]
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/tools/flow/CSSModule.js.flow'
module.name_mapper.extension='styl' -> '<PROJECT_ROOT>/tools/flow/CSSModule.js.flow'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/tools/flow/CSSModule.js.flow'
module.name_mapper.extension='sass' -> '<PROJECT_ROOT>/tools/flow/CSSModule.js.flow'
module.name_mapper.extension='less' -> '<PROJECT_ROOT>/tools/flow/CSSModule.js.flow'

module.name_mapper.extension='png' -> '<PROJECT_ROOT>/tools/flow/WebpackAsset.js.flow'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/tools/flow/WebpackAsset.js.flow'

esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
