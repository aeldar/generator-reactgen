  "scripts": {
    "eslint": "eslint --ext .js,.jsx src --fix",
    "stylelint": "stylelint src/**/*.scss",
    "csscomb": "csscomb src",
    "lint": "npm run eslint && npm run stylelint",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "clean": "npm run clean:reports && rimraf build",
    "clean:reports": "rimraf reports",
    "build": "npm run clean && NODE_ENV=production webpack --progress",
    "start":
      "npm run clean && NODE_ENV=development webpack-dev-server --hot --inline --progress",
    "test":
      "npm run clean:reports && NODE_ENV=test karma start --single-run --no-auto-watch",
    "test:watch": "NODE_ENV=test karma start --no-single-run --auto-watch",
    "test:mocha": "mocha --compilers js:babel-register",
    "selenium-install": "webdriver-manager update",
    "e2e": "npm run nightwatch",
    "e2e:firefox": "npm run nightwatch -- --env firefox",
    "e2e:chrome": "npm run nightwatch -- --env chrome",
    "nightwatch": "cd test/nightwatch/ && nightwatch"
  },
